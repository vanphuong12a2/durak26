{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules": {
    "no-console": [false],
    "ordered-imports": [false],
    "quotemark": [true, "single"],
    "interface-name": [false],
    "object-literal-sort-keys": [false],
    "no-namespace": false
  },
  "linterOptions": {
    "exclude": [
      "node_modules/**/*.ts",
      "src/serviceWorker.js"
    ]
  }
}
